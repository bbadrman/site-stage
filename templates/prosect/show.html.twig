{% extends 'base.html.twig' %}

{% block title %}Prosect{% endblock %}

{% block body %}
    <h1>Prosect</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prosect.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ prosect.name }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ prosect.lastname }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ prosect.email }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ prosect.tel }}</td>
            </tr>
            <tr>
                <th>Datenaissane</th>
                <td>{{ prosect.datenaissane ? prosect.datenaissane|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_prosect_index') }}">back to list</a>

    <a href="{{ path('app_prosect_edit', {'id': prosect.id}) }}">edit</a>

    {{ include('prosect/_delete_form.html.twig') }}
{% endblock %}
